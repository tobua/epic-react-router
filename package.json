{
  "name": "epic-react-router",
  "version": "1.0.0",
  "description": "MobX react router.",
  "repository": "github:tobua/epic-react-router",
  "license": "MIT",
  "main": "dist/index.js",
  "source": "index.tsx",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "padua watch",
    "test": "padua test"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs",
    "rules": {
      "@typescript-eslint/lines-between-class-members": 0
    }
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "./node_modules/padua/configuration/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": [
        "babel-jest",
        {
          "configFile": "./node_modules/padua/configuration/.babelrc"
        }
      ]
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ]
  },
  "dependencies": {
    "history": "^5.0.0",
    "query-string": "^6.13.1"
  },
  "devDependencies": {
    "@types/history": "^4.7.7",
    "@types/react": "^16.9.49",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "padua": "^0.0.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1"
  },
  "peerDependencies": {
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "react": "^16.13.1"
  }
}
